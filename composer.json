{
    "name": "troshyn/kak-clickhouse-fork",
    "description": "ClickHouse for Yii2. fork",
    "type": "yii2-extension",
    "keywords": ["yii2","ClickHouse","ClickHouse ActiveRecord Yii2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tutik Alexsandr",
            "email": "sanchezzzhak@ya.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-httpclient": "^2.0.14"
    },
    "autoload": {
        "psr-4": {
            "kak\\clickhouse\\": ""
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
